<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>br.com.objectos</groupId>
		<artifactId>parent</artifactId>
		<version>39</version>
	</parent>

	<artifactId>orm-parent</artifactId>
	<version>0.1.0-SNAPSHOT</version>
	<packaging>pom</packaging>

	<name>objectos :: orm :: parent</name>
	<description>A lightweight Java8 ORM library.</description>
	<url>https://github.com/objectos/orm</url>

	<licenses>
		<license>
			<name>The Apache Software License, Version 2.0</name>
			<url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
			<distribution>repo</distribution>
		</license>
	</licenses>

	<scm>
		<url>https://github.com/objectos/orm</url>
		<connection>scm:git:git://objectos/orm.git</connection>
		<developerConnection>scm:git:git@github.com:objectos/orm.git</developerConnection>
		<tag>HEAD</tag>
	</scm>

	<issueManagement>
		<system>GitHub Issues</system>
		<url>http://github.com/objectos/orm/issues</url>
	</issueManagement>

	<developers>
		<developer>
			<id>moe</id>
			<name>Marcio Endo</name>
			<email>marcio.endo@objectos.com.br</email>
		</developer>
	</developers>

	<organization>
		<name>Objectos Software LTDA</name>
		<url>http://www.objectos.com.br</url>
	</organization>

	<modules>
		<module>orm</module>
		<module>orm-compiler</module>
		<module>orm-it</module>
	</modules>

	<properties>
		<objectos.assertion.version>0.1.1</objectos.assertion.version>
		<objectos.code.version>0.8.1</objectos.code.version>
		<objectos.collections.version>0.1.1</objectos.collections.version>
		<objectos.metainf.version>0.1.0</objectos.metainf.version>
		<objectos.pojo.version>0.5.3</objectos.pojo.version>
		<objectos.schema.version>0.1.1-SNAPSHOT</objectos.schema.version>
		<objectos.testable.version>0.4.2</objectos.testable.version>
	</properties>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>br.com.objectos</groupId>
				<artifactId>assertion</artifactId>
				<version>${objectos.assertion.version}</version>
				<scope>test</scope>
				<exclusions>
					<exclusion>
						<groupId>br.com.objectos</groupId>
						<artifactId>core</artifactId>
					</exclusion>
					<exclusion>
						<groupId>com.google.guava</groupId>
						<artifactId>guava</artifactId>
					</exclusion>
				</exclusions>
			</dependency>
			<dependency>
				<groupId>br.com.objectos</groupId>
				<artifactId>code-apt</artifactId>
				<version>${objectos.code.version}</version>
			</dependency>
			<dependency>
				<groupId>br.com.objectos</groupId>
				<artifactId>collections</artifactId>
				<version>${objectos.collections.version}</version>
			</dependency>
			<dependency>
				<groupId>br.com.objectos</groupId>
				<artifactId>orm</artifactId>
				<version>0.1.0-SNAPSHOT</version>
			</dependency>
			<dependency>
				<groupId>br.com.objectos</groupId>
				<artifactId>orm-compiler</artifactId>
				<version>0.1.0-SNAPSHOT</version>
			</dependency>
			<dependency>
				<groupId>br.com.objectos</groupId>
				<artifactId>metainf-compiler</artifactId>
				<version>${objectos.metainf.version}</version>
			</dependency>
			<dependency>
				<groupId>br.com.objectos</groupId>
				<artifactId>pojo-compiler</artifactId>
				<version>${objectos.pojo.version}</version>
			</dependency>
			<dependency>
				<groupId>br.com.objectos</groupId>
				<artifactId>pojo-plugin</artifactId>
				<version>${objectos.pojo.version}</version>
			</dependency>
			<dependency>
				<groupId>br.com.objectos</groupId>
				<artifactId>pojo-testing</artifactId>
				<version>${objectos.pojo.version}</version>
				<scope>test</scope>
			</dependency>
			<dependency>
				<groupId>br.com.objectos</groupId>
				<artifactId>schema-info</artifactId>
				<version>${objectos.schema.version}</version>
			</dependency>
			<dependency>
				<groupId>br.com.objectos</groupId>
				<artifactId>schema-info-testing</artifactId>
				<version>${objectos.schema.version}</version>
				<scope>test</scope>
			</dependency>
			<dependency>
				<groupId>br.com.objectos</groupId>
				<artifactId>testable</artifactId>
				<version>${objectos.testable.version}</version>
			</dependency>
			<dependency>
				<groupId>br.com.objectos</groupId>
				<artifactId>testable-pojo-plugin</artifactId>
				<version>${objectos.testable.version}</version>
			</dependency>
		</dependencies>
	</dependencyManagement>

</project>
